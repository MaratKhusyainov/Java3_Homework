<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Hometask6.Chat.AuthDialog"
            prefHeight="247.0" prefWidth="361.0">
    <children>
        <TextField fx:id="userName" layoutX="165.0" layoutY="54.0" />
        <Label layoutX="39.0" layoutY="58.0" text="Login" />
        <Label layoutX="39.0" layoutY="116.0" text="Password" />
        <PasswordField fx:id="userPassword" layoutX="165.0" layoutY="112.0" onAction="#submitUserPassword" />
        <Button fx:id="buttonOk" layoutX="96.0" layoutY="200.0" mnemonicParsing="false" text="OK" onAction="#submitUserPassword"/>
        <Button layoutX="205.0" layoutY="200.0" mnemonicParsing="false" text="Cancel" onAction="#exit" />
    </children>

</AnchorPane>
